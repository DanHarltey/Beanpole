<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
  <appSettings>
    <!-- needed for log4net, this tells it that the config is in another file -->
    <add key="log4net.Config" value="Log4Net.config"/>
    <add key="log4net.Config.Watch" value="True"/>

    <add key="File" value="C:\inetpub\wwwroot\web.config"/>
    <add key="WebAddress" value="http://someaddress"/>
    <add key="XmlEndPointAddress" value="http://someaddress/Config/Xml?Environment="/>
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>